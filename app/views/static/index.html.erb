<div ng-controller="SortableCtrl">
<ul ui-sortable ng-model="items">
    <li ng-repeat="item in items">{{ item }}</li>
</ul>
</div>
<div ng-controller="CalendarCtrl">
  <button class="btn btn-success" ng-click="changeView('agendaDay', myCalendar1)">AgendaDay</button>
  <button class="btn btn-success" ng-click="changeView('agendaWeek', myCalendar1)">AgendaWeek</button>
  <button class="btn btn-success" ng-click="changeView('month', myCalendar1)">AgendaMonth</button>
  <div ui-calendar="uiConfig.calendar" class="span8 calendar" calendar="myCalendar1" ng-model="eventSources"></div>
</div>
<div ng-controller="AccordionDemoCtrl">
  <label class="checkbox">
    <input type="checkbox" ng-model="oneAtATime">
      Open only one at a time
    </label>

    <accordion close-others="oneAtATime">
       <accordion-group heading="Static Header, initially expanded" is-open="true">
          This content is straight in the template.
       </accordion-group>
       <accordion-group heading="{{group.title}}" ng-repeat="group in groups">
         {{group.content}}
       </accordion-group>
       <accordion-group heading="Dynamic Body Content">
         <p>The body of the accordion group grows to fit the contents</p>
         <button class="btn btn-default btn-sm" ng-click="addItem()">Add Item</button>
         <div ng-repeat="item in items">{{item}}</div>
       </accordion-group>
       <accordion-group is-open="isopen">
          <accordion-heading>
          I can have markup, too! <i class="pull-right glyphicon" ng-class="{'glyphicon-chevron-down': isopen, 'glyphicon-chevron-right': !isopen}"></i>
          </accordion-heading>
           This is just some content to illustrate fancy headings.
       </accordion-group>
  </accordion>
</div>
<div class="container" ng-controller="Ctrl">
  <div class="row">
    <div class="col-sm-12">
      <div class="gridStyle" ng-grid="gridOptions">
